{
  "name": "pwa-webshop",
  "version": "1.0.0",
  "description": " ",
  "private": true,
  "nyc": {
    "exclude": [
      "**/*.mock.js",
      "**/*.test.js",
      "**/tests",
      "**/helpers",
      "**/unit/common",
      "**/async-components-list.js"
    ],
    "extension": [
      ".js",
      ".vue"
    ]
  },
  "scripts": {
    "dev": "cross-env CONFIG=dev nuxt",
    "e2e-env": "cross-env CONFIG=test nuxt",
    "dev:en": "cross-env LOCALE=en-US STORE_ID=49 nuxt",
    "dev:ie": "cross-env LOCALE=en-IE CONFIG=dev nuxt",
    "dev:prod": "cross-env CONFIG=prod nuxt",
    "dev:au": "cross-env LOCALE=en-AU CONFIG=dev nuxt",
    "dev:debug": "cross-env NUXT_HOST=0.0.0.0 nuxt",
    "build": "nuxt build",
    "build-dev": "cross-env CONFIG=dev nuxt build",
    "build-prod": "cross-env CONFIG=prod nuxt build",
    "start": "nuxt start",
    "start-prod": "nuxt build && nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js --ext .vue src/",
    "lint:css": "stylelint \"src/**/*.vue\" \"src/assets/styles/app/**/*.scss\"",
    "docker": "npm run build && npm run start",
    "update-snapshots": "ava --update-snapshots",
    "test-unit": "nyc --reporter=html --reporter=text ava",
    "test": "nyc --reporter=html --reporter=clover --report-dir=tests/results ava --tap | tap-xunit > tests/results/api-junit-test.xml || exit 0",
    "test-unit:watch": "ava --watch",
    "test-e2e": "node src/tests/e2e/config/runner.js -c src/tests/e2e/config/index.js -e win10_chrome61,win10_chrome59,win10_firefox56,win10_firefox55,win10_firefox51,win10_edge15,win10_ie11,win81_chrome61,win81_firefox56,win81_ie11,win7_chrome61,win7_firefox56,win7_firefox55,win7_firefox52,win7_ie11,winXP_chrome49,osx_safari9,osx_safari10,osx_chrome49,osx_chrome52,osx_chrome60,osx_chrome61",
    "test-e2e:local": "node_modules/.bin/nightwatch --config ./src/tests/e2e/config/nightwatch.conf.LOCAL.js",
    "eventBusValidator": "run-func \"./src/helpers/eventBusSchemaGenerator.js\" generateEventBusSchema",
    "update-npm": "npx rimraf package-lock.json && npx rimraf node_modules && npm i",
    "create-csv": "node src/helpers/createCSV.js",
    "generate-multilanguage-countries": "node src/helpers/generateCountiesWithMultipleLanguages.js",
    "import-translations": "rm -rf src/common/locales/* && node src/helpers/importTranslations.js",
    "auto-commit-translations": "./update-translations.sh"
  },
  "engines": {
    "node": ">=14.0.0 <15.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint:js && npm run lint:css && npm run eventBusValidator && npm run test-unit",
      "post-commit": "npm run auto-commit-translations"
    }
  },
  "dependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "7.9.6",
    "@babel/register": "7.9.0",
    "@babel/runtime-corejs3": "7.9.6",
    "@nuxtjs/axios": "^5.10.3",
    "@nuxtjs/device": "^1.2.7",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "2.6.0",
    "@nuxtjs/sentry": "^3.0.1",
    "@nuxtjs/svg": "^0.1.11",
    "axios": "^0.19.1",
    "babel-plugin-add-module-exports": "1.0.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-imports": "1.5.1",
    "body-parser": "^1.19.0",
    "core-js": "^3.2.1",
    "es-abstract": "1.14.2",
    "es6-promise": "4.2.4",
    "fs-extra": "7.0.0",
    "glob-promise": "3.4.0",
    "intersection-observer": "0.7.0",
    "intl-messageformat": "2.2.0",
    "jsdom": "11.11.0",
    "lodash": "4.17.15",
    "node-country-list": "^1.0.3",
    "node-sass": "^4.14.1",
    "nuxt": "2.9.1",
    "require-extension-hooks": "0.3.3",
    "require-extension-hooks-babel": "0.1.1",
    "require-extension-hooks-vue": "1.1.0",
    "rewire": "4.0.1",
    "sass-loader": "7.1.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "swiper": "^5.3.6",
    "v-clipboard": "^2.2.3",
    "vue-click-outside": "1.0.7",
    "vue-loader": "15.4.2",
    "vue-youtube": "^1.4.0",
    "vue-zoom-on-hover": "1.0.2",
    "vuetify": "1.5.4",
    "vuex-persistedstate": "2.5.4",
    "vuex-shared-mutations": "^1.0.2",
    "webpack-plugin-hash-output": "^3.2.1"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "0.2.0",
    "@nuxt/loading-screen": "0.5.2",
    "@vue/test-utils": "1.0.0-beta.25",
    "ava": "2.4.0",
    "babel-eslint": "10.0.1",
    "browser-env": "3.2.5",
    "browserstack-local": "1.3.4",
    "chromedriver": "2.43.0",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.3",
    "esm": "^3.2.25",
    "execa": "1.0.0",
    "husky": "1.0.0",
    "js-beautify": "1.10.0",
    "nightwatch": "1.1.13",
    "nyc": "14.1.1",
    "proxyquire": "2.1.0",
    "run-func": "1.0.2",
    "selenium-server": "3.14.0",
    "sinon": "6.3.4",
    "stylelint": "10.1.0",
    "stylelint-config-recess-order": "2.0.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-webpack-plugin": "0.10.5",
    "tap-xunit": "2.3.0"
  }
}
